        {
            "name": "Amirreza",
            "email": "amirreza.firoz13@gmail.com",
            "homepage": "https://AmirrezaFiroz.github.io"
        },
        {
            "name": "ALi",
            "email": "alishahbazpor110@gmail.com"
        }
    ],
    "require": {
        "symfony/yaml": "^6.2",
        "textalk/websocket": "^1.6",
        "php": ">=8",
        "james-heinrich/getid3": "^1.9"
    },
    "homepage": "https://rubika-library.github.io"
}