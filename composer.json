    "type": "library",
    "version": "v1.2.0",
    "autoload": {
        "psr-4": {
            "Rubika\\": "./"
        },
        "files": [
            "helper/Fast.php",
            "helper/Web.php"
        ],
        "exclude-from-classmap": [
            "Docker/"
        ]
    },
    "readme": "README.md",
